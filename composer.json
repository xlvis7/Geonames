{
  "name": "xlvis7/geonames",
  "description": "A Laravel (php) package that interfaces with the geolocation services on geonames.org.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "xlvis7 (contact me @github)",
      "email": "xl@xl.xl"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-intl": "*",
    "ext-pdo": "*",
    "ext-curl": "*",
    "laravel/framework": "^7.0",
    "michaeldrennen/remote-file": "^2.0",
    "michaeldrennen/local-file": "^2.0",
    "curl/curl": "^2.2",
    "fabpot/goutte": "^4.0",
    "nesbot/carbon": "^2.28",
    "illuminate/routing": "^7.0",
    "illuminate/support": "^7.0",
    "illuminate/database": "^7.0"

  },
  "require-dev": {
    "orchestra/testbench": "^3.8",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpunit": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "MichaelDrennen\\Geonames\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelDrennen\\Geonames\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "MichaelDrennen\\Geonames\\GeonamesServiceProvider"
      ]
    }
  }
}
